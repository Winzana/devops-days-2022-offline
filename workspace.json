    "website": {
      "root": "apps/website",
      "sourceRoot": "apps/website/src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@nrwl/web:webpack",
          "outputs": ["{options.outputPath}"],
          "defaultConfiguration": "production",
          "options": {
            "compiler": "babel",
            "outputPath": "dist/apps/website",
            "index": "apps/website/src/index.html",
            "baseHref": "/",
            "main": "apps/website/src/main.tsx",
            "polyfills": "apps/website/src/polyfills.ts",
            "tsConfig": "apps/website/tsconfig.app.json",
            "assets": [
              "apps/website/src/favicon.ico",
              "apps/website/src/manifest.json",
              "apps/website/src/assets"
            ],
            "styles": ["apps/website/src/styles.scss"],
            "scripts": [],
            "webpackConfig": "apps/website/webpack.config.js"
          },
          "configurations": {
            "development": {
              "extractLicenses": false,
              "optimization": false,
              "sourceMap": true,
              "vendorChunk": true
            },
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/website/src/environments/environment.ts",
                  "with": "apps/website/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@nrwl/web:dev-server",
          "defaultConfiguration": "development",
          "options": {
            "buildTarget": "website:build",
            "hmr": true
          },
          "configurations": {
            "development": {
              "buildTarget": "website:build:development"
            },
            "production": {
              "buildTarget": "website:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["apps/website/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/website"],
          "options": {
            "jestConfig": "apps/website/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
